{
  "name": "coc-angular",
  "description": "Editor services for Angular templates",
  "version": "0.1.0",
  "keywords": [
    "coc.nvim",
    "angular",
    "multi-root ready"
  ],
  "author": {
    "name": "iamcco",
    "email": "ooiss@qq.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iamcco/coc-angular"
  },
  "engines": {
    "coc": "^0.0.62"
  },
  "activationEvents": [
    "onLanguage:ng-template",
    "onLanguage:html",
    "onLanguage:typescript"
  ],
  "main": "./lib/index.js",
  "contributes": {
    "configuration": {
      "title": "angular configuration",
      "properties": {
        "angular.debug": {
          "type": "boolean",
          "default": false,
          "description": "enable coc-angular service debug"
        },
        "angular.trace.server": {
          "type": "boolean",
          "default": false,
          "description": "enable angular language server trace log?"
        }
      }
    }
  },
  "scripts": {
    "build": "tsc -p ./",
    "watch": "tsc -w -p ./"
  },
  "devDependencies": {
    "@types/node": "^10.9.4",
    "coc.nvim": "^0.0.72",
    "typescript": "~3.4.2"
  },
  "dependencies": {
    "angular-lsp-service": "^0.1.2"
  }
}
